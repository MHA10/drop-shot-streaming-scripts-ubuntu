{
  "name": "nodejs-stream-manager",
  "version": "1.0.0",
  "description": "Node.js/TypeScript stream manager for RTSP to RTMP streaming on Raspberry Pi",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "keywords": [
    "streaming",
    "rtsp",
    "rtmp",
    "ffmpeg",
    "raspberry-pi",
    "typescript"
  ],
  "author": "Stream Manager",
  "license": "MIT",
  "dependencies": {
    "eventsource": "^2.0.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/eventsource": "^1.1.15",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}